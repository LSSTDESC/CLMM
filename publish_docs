git fetch origin
git merge origin/main
python setup.py install --user > /dev/null

cd docs/
make clean
sed -i -e 's/run_nb = False/run_nb = True/g' conf.py
make html
sed -i -e 's/run_nb = True/run_nb = False/g' conf.py
cd ..

MASTERCOMMIT="$(git log --pretty=format:'%h' -n 1 main)"

git add docs/_build/
git status
git commit -m "Added main commit $MASTERCOMMIT to docs"
git push
git subtree push --prefix docs/_build/html/ origin gh-pages
